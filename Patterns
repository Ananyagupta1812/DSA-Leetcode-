#include <iostream>
using namespace std;

//SQUARE PATTERN
int main() {
    int n;
    cin >>n;

    for (int i=1; i<=n ; i++){
        for (int j=1; j<=n; j++){
            cout<<j<<" ";
        }
        cout<<endl;
    }

    return 0;
}

//CHARCATER SQUARE 
int main() {
    int n;
    cin >>n;

    for (int i=0; i<n ; i++){
        char ch= 'A';
        for (int j=0; j<n; j++){
            cout<<ch<<" ";
            ch=ch+1; //ascii value of A is 65-> here 65+1=66, now to get stored in a char 
            //variable again implicit type conversion will happen and it will be vhanged to 'B'
            //OR ch++;
        }
        cout<<endl;
    }

    return 0;
}

//INCREASING NUMBERS
int main() {
    int n;
    cin >>n;
    int num =1; 

    for (int i=0; i<n ; i++){
        for (int j=0; j<n; j++){
            cout<<num;
            num++;
        }
        cout<<endl;
    }

    return 0;
}

//INCREASING CHARACTERS
int main() {
    int n;
    cin >>n;
    char ch ='A'; 

    for (int i=0; i<n ; i++){
        for (int j=0; j<n; j++){
            cout<<ch;
            ch++;
        }
        cout<<endl;
    }

    return 0;
}

//TRIANGLE PATTERN 
int main() {
    int n;
    cin >>n;; 

    for (int i=0; i<n ; i++){
        for (int j=0; j<i+1; j++){
            cout<<"* ";
        }
        cout<<endl;
    }

    return 0;
}

//TRIANGLE PATTERN WITH INCREASING NUMBERS 1 
int main() {
    int n;
    cin >>n;
    int var=1;

    for (int i=0; i<n ; i++){
        for (int j=0; j<i+1; j++){
            cout<<var;
        }
        var++;
        cout<<endl;
    }

    return 0;
}

//TRIANGLE PATTERN WITH INCREASING NUMBERS 2
int main() {
    int n;
    cin >>n;
    int var=1;
    for (int i=0; i<n ; i++){
        for (int j=0; j<i+1; j++){
            cout<<var<<" ";
            var++;
        }
        cout<<endl;
    }

    return 0;
}

//REVERSE TRIANGLE 1 
int main() {
    int n;
    cin >>n;
    //char ch ='A';

    for (int i=0; i<n ; i++){
        //int var=1;
        for (int j=i+1; j>0; j--){
            cout<<j<<" ";
            //var++;
        }
        //ch++;
        cout<<endl;
    }

    return 0;
}

// REVERSE TRIANGLE 2
int main() {
    int n=5;
    //char ch ='A';

    for (int i=n; i>=0 ; i--){
        //int var=1;
        for (int j=i; j>0; j--){
            cout<<j<<" ";
            //var++;
        }
        //ch++;
        cout<<endl;
    }

    return 0;
}

//FLOYD'S TRIANGLE 
int main() {
    int n=5;
    //char ch ='A';
    int var=1;

    for (int i=0; i<n ; i++){
        for (int j=0; j<i+1; j++){
            cout<<var<<" ";
            var++;
        }
        //ch++;
        cout<<endl;
    }

    return 0;
}

//INVERTED TRIANGLE PATTERN 
int main() {
    int n=5;

    for (int i=0; i<n ; i++){
        for (int j=0; j<i+1; j++){
            cout<<" ";
        }
        for(int j=0; j<n-i; j++){
            cout<<i+1;
        }
        cout<<endl;
    }

    return 0;
}

//PYRAMID PATTERN 
int main() {
    int n=5;

    for (int i=0; i<n ; i++){
        for (int j=0; j<n-i-1; j++){
            cout<<" ";
        }
        for(int j=1; j<i+1; j++){
            cout<<j;
        }
        for(int j=i-1; j>=1; j--){
            cout<<j;
        }
        cout<<endl;
    }
 
    return 0;
}
